{
  "mcpServers": {
    "serena": {
      "type": "stdio",
      "command": "serena",
      "args": [
        "start-mcp-server",
        "--transport",
        "stdio",
        "--project",
        "."
      ],
      "env": {}
    },
    "cratedocs": {
      "type": "stdio",
      "command": "cratedocs",
      "args": [
        "stdio"
      ],
      "env": {}
    },
    "cargo": {
      "type": "stdio",
      "command": "cargo-mcp",
      "args": [
        "serve"
      ],
      "env": {}
    },
    "claude-task-master": {
      "args": [],
      "command": "task-master-mcp",
      "env": {
        "TASKMASTER_DIR": ".taskmaster"
      },
      "type": "stdio"
    },
    "codanna": {
      "args": [
        "serve",
        "--watch",
        "--watch-interval",
        "5"
      ],
      "command": "codanna",
      "type": "stdio"
    }
  }
}
